{
  "name": "nextjs-base",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:js": "eslint --ext \".js,.jsx,.ts,.tsx\" --ignore-path .gitignore .",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "lint:css": "stylelint --fix **/*.{css,scss,sass} --ignore-path .gitignore --config-basedir ./src/assets/styles/",
    "lint:staged": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "18.15.11",
    "@types/react": "18.0.35",
    "@types/react-dom": "18.0.11",
    "autoprefixer": "10.4.14",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "iron-session": "^6.3.1",
    "lodash": "^4.17.21",
    "next": "13.3.0",
    "next-iron-session": "^4.2.0",
    "next-redirects": "^3.2.3",
    "next-translate": "^2.0.5",
    "postcss": "8.4.22",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "recoil": "^0.7.7",
    "recoil-nexus": "^0.4.0",
    "sass": "^1.62.0",
    "status-code-enum": "^1.0.0",
    "tailwindcss": "3.3.1",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@types/lodash": "^4.14.194",
    "eslint-config-prettier": "^8.8.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "next-translate-plugin": "^2.0.5",
    "prettier": "^2.8.7",
    "stylelint": "^15.5.0",
    "stylelint-config-recommended-scss": "^10.0.0",
    "stylelint-config-standard-scss": "^8.0.0",
    "stylelint-prettier": "^3.0.0",
    "stylelint-scss": "^4.6.0",
    "stylelint-selector-bem-pattern": "^2.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
